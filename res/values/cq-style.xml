<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="LabelText" parent="@android:style/TextAppearance.Small">
		<item name="android:gravity">center_horizontal|center_vertical</item>
		<item name="android:layout_width">80dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:textColor">#000000</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="DisplayText" parent="@android:style/TextAppearance.Small">
		<item name="android:textColor">#ffffff</item>
	</style>
	<style name="BlackDisplayText" parent="@android:style/TextAppearance.Small">
		<item name="android:textColor">#000000</item>
	</style>
	
	<style name="TabbarOuterButton">
        <item name="android:background">@null</item>		
		<item name="android:layout_weight">0</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_height">80dp</item> 
	    <item name="android:layout_width">74dp</item>
	</style>
	<style name="TitlebarOuterButton">
        <item name="android:background">@null</item>		
		<item name="android:layout_weight">0</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_height">40dp</item> 
	    <item name="android:layout_width">41dp</item>
	</style>
	<style name="GrayButton" parent="@android:style/Widget.Button">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:textColor">@drawable/white</item>
		<item name="android:background">@drawable/cq_gray_btnx</item>
		<item name="android:gravity">center_vertical|center_horizontal</item>
	</style>
	<style name="ContactDetailsRow">
		<item name="android:layout_width">fill_parent</item> 
		<item name="android:layout_height">50dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:background">@drawable/cq_box_bg_middle</item>
		<item name="android:orientation">horizontal</item>
	</style>
	<style name="ContactDetailHeaderCol">
		<item name="android:layout_width">60dp</item> 
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">#009ACD</item>
		<item name="android:textSize">14dp</item>
		<item name="android:layout_marginLeft">20dp</item>
	</style>
	<style name="ContactDetailsDetailCol">
		<item name="android:layout_width">wrap_content</item> 
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14dp</item>
		<item name="android:layout_marginLeft">20dp</item>
		<item name="android:textColor">#000000</item>
		<item name="android:layout_weight">0.5</item>
	</style>
	
	<style name="CQImageOnlyButton">
		<item name="android:background">@null</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_height">70dp</item>
		<item name="android:layout_width">70dp</item>
	</style>
	<style name = "CQCheckmarkImage">
		<item name="android:src">@drawable/cq_checkmark</item>
		<item name="android:layout_height">22dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_width">22dp</item>
		<item name="android:layout_gravity">center_vertical</item>
	</style>
	<style name="CQDefaultPhotoSize">
		<item name="android:layout_width">25dp</item>
		<item name="android:layout_height">25dp</item>
	</style>
</resources>
